{
  "extends": [
    "next/core-web-vitals",
    "plugin:@next/next/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "camelcase": "off",
    "consistent-return": "warn",
    "eqeqeq": [
      "warn",
      "always"
    ],
    "func-style": [
      "warn",
      "expression"
    ],
    "indent": [
      "warn",
      2
    ],
    "keyword-spacing": [
      "warn",
      {
        "before": true
      }
    ]
  }
}